<h1><%= @student_name %>'s Incomplete Assignments</h1>
  <% if @inc.empty? %>
    <h3>All work completed!!!     Nothing outstanding!!     Good job, <%= @student_name %>!</h3>
  <% else %>
    <% @inc.each do |incomplete| %>
      <strong><%= incomplete.assignment.subject.name %></strong><br><br>
      <%= incomplete.assignment.assignment_type %>        <%= incomplete.assignment.created_at.strftime("%m/%d/%Y") %><br>

      <%= incomplete.assignment.notes%><br>

      <%= button_to "Submit Assignment", students_assignment_path(incomplete), :method => :put %>

    <% end %>
  <% end %>
